local Players = game:GetService("Players")

local function playersContentFunc()
	local function pgui(var)

		local player = Instance.new("Frame")
		local playerIcon = Instance.new("ImageLabel")
		local pI = Instance.new("UICorner")
		local playerName = Instance.new("TextLabel")
		local pN = Instance.new("UIPadding")
		local pNpN = Instance.new("UITextSizeConstraint")
		local selectButton = Instance.new("TextButton")
		local playerSelected = Instance.new("TextLabel")
		local UIPadding = Instance.new("UIPadding")
		player.Name = var.Name
		player.Parent = playersContent.content
		player.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		player.BackgroundTransparency = 1.000
		player.BorderColor3 = Color3.fromRGB(0, 0, 0)
		player.BorderSizePixel = 0
		player.Size = UDim2.new(1, 0, 0, 40)
		player.Visible = true
		playerIcon.Name = "playerIcon"
		playerIcon.Parent = player
		playerIcon.AnchorPoint = Vector2.new(0, 0.5)
		playerIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		playerIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
		playerIcon.BorderSizePixel = 0
		playerIcon.Position = UDim2.new(0, 0, 0.5, 0)
		playerIcon.Size = UDim2.new(0, 40, 0, 40)
		playerIcon.Image = game.Players:GetUserThumbnailAsync(var.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
		pI.CornerRadius = UDim.new(1, 0)
		pI.Name = "pI"
		pI.Parent = playerIcon
		playerName.Name = "playerName"
		playerName.Parent = player
		playerName.AnchorPoint = Vector2.new(0, 0.5)
		playerName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		playerName.BackgroundTransparency = 1.000
		playerName.BorderColor3 = Color3.fromRGB(0, 0, 0)
		playerName.BorderSizePixel = 0
		playerName.Position = UDim2.new(0, 40, 0.5, 0)
		playerName.Size = UDim2.new(0, 100, 0, 40)
		playerName.Font = Enum.Font.Gotham
		playerName.Text = "("..var.DisplayName..")\n "..var.Name
		playerName.TextColor3 = Color3.fromRGB(216, 222, 233)
		playerName.TextScaled = true
		playerName.TextSize = 13.000
		playerName.TextWrapped = true
		playerName.TextXAlignment = Enum.TextXAlignment.Left
		pN.Name = "pN"
		pN.Parent = playerName
		pN.PaddingLeft = UDim.new(0, 10)
		pNpN.Name = "pNpN"
		pNpN.Parent = playerName
		pNpN.MaxTextSize = 13
		selectButton.Name = "selectButton"
		selectButton.Parent = player
		selectButton.AnchorPoint = Vector2.new(0.5, 0.5)
		selectButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		selectButton.BackgroundTransparency = 1.000
		selectButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		selectButton.BorderSizePixel = 0
		selectButton.Position = UDim2.new(0.5, 0, 0.5, 0)
		selectButton.Size = UDim2.new(1, 0, 1, 0)
		selectButton.Font = Enum.Font.SourceSans
		selectButton.Text = ""
		selectButton.TextColor3 = Color3.fromRGB(0, 0, 0)
		selectButton.TextSize = 14.000
		selectButton.TextXAlignment = Enum.TextXAlignment.Left
		playerSelected.Name = "playerSelected"
		playerSelected.Parent = player
		playerSelected.AnchorPoint = Vector2.new(0, 0.5)
		playerSelected.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		playerSelected.BackgroundTransparency = 1.000
		playerSelected.BorderColor3 = Color3.fromRGB(0, 0, 0)
		playerSelected.BorderSizePixel = 0
		playerSelected.Position = UDim2.new(0, 140, 0.5, 0)
		playerSelected.Size = UDim2.new(0, 111, 1, 0)
		playerSelected.Font = Enum.Font.GothamMedium
		playerSelected.Text = "Unselected"
		playerSelected.TextColor3 = Color3.fromRGB(190, 100, 105)
		playerSelected.TextSize = 14.000
		UIPadding.Parent = player
		UIPadding.PaddingLeft = UDim.new(0, 5)

		local ss = false
		selectButton.MouseButton1Click:Connect(function()
			local n = string.split(playerName.Text, ")\n ")
			ss = not ss
			if ss then
				local stringValue = Instance.new("StringValue")
				stringValue.Parent = Selected
				stringValue.Name = "Player: "..n[2]
				stringValue.Value = tostring(n[2])
				playerSelected.Text = "Selected"
				u:tween({o = playerSelected,
					a = {TextColor3 = Color3.fromRGB(85, 220, 135)},
					d = "In",s = "Sine",t = 0.1
				}):Play()
			else
				for i,v in pairs(Selected:GetChildren()) do
					if v.Value == tostring(n[2]) then
						v:Destroy()
					end
				end
				playerSelected.Text = "Unselected"
				u:tween({o = playerSelected,
					a = {TextColor3 = Color3.fromRGB(190, 100, 105)},
					d = "In",s = "Sine",t = 0.1
				}):Play()
			end
		end)
	end

	for i,player in ipairs(Players:GetChildren()) do
		if player.Name ~= game.Players.LocalPlayer.Name then
			pgui(player)
		end
	end

	Players.PlayerAdded:Connect(function(player)
		pgui(player)
	end)

	Players.PlayerRemoving:Connect(function(player)
		for i,v in ipairs(playersContent.content:GetChildren()) do
			if v.Name == player.Name then
				v:Destroy()
			end
		end
	end)
end
playersContentFunc()

local function buttonsFunc(buttons)
	for i,frame in ipairs(buttons) do
		for i,v in ipairs(frame:GetDescendants()) do
			if v:IsA("TextButton") then
				v.MouseButton1Down:Connect(function()
					u:tween({o = v.Parent,
						a = {BackgroundTransparency = 0.9},
						d = "In",s = "Sine",t = 0.1
					}):Play()
				end)
				v.MouseButton1Up:Connect(function()
					u:tween({o = v.Parent,
						a = {BackgroundTransparency = 0.8},
						d = "In",s = "Sine",t = 0.1
					}):Play()
				end)
				v.MouseLeave:Connect(function()
					u:tween({o = v.Parent,
						a = {BackgroundTransparency = 0.8},
						d = "In",s = "Sine",t = 0.1
					}):Play()
				end)
			end
		end
	end
end
buttonsFunc({buttons,miscButtons,fireGamepass})

for i,v in ipairs(tabs:GetDescendants()) do
	if v:IsA("TextButton") then
		v.MouseButton1Click:Connect(function()
			for i2,v2 in ipairs(tabs:GetDescendants()) do
				if v2:IsA("TextButton") then
					u:tween({o = v2,
						a = {TextTransparency = 0.5},
						d = "In",s = "Sine",t = 0.2
					}):Play()
					u:tween({o = v2.image,
						a = {ImageTransparency = 0.5},
						d = "In",s = "Sine",t = 0.2
					}):Play()
				end
			end
			u:tween({o = v,
				a = {TextTransparency = 0},
				d = "In",s = "Sine",t = 0.2
			}):Play()
			u:tween({o = v.image,
				a = {ImageTransparency = 0},
				d = "In",s = "Sine",t = 0.2
			}):Play()
			mC:JumpTo(mainContent[v.Parent.Name.."Window"])
		end)
	end
end

--buttons n stuff

local RS = game:GetService("RunService")

local function shootWand(x,y,z,damage)
	local lp = game.Players.LocalPlayer
	local char = lp.Character
	local wand = char:FindFirstChild("Wand")
	if not wand then
		local WandB = lp.Backpack.Wand
		char.Humanoid:EquipTool(WandB)
		wand = char.Wand
	else

	end
	local info = {
		CFrame.new(x, y, z), 
		0, 
		0.1,
		wand, 
		damage, 
		char
	}
	wand.Fire:FireServer(unpack(info))
end

local function teamCheck(plyr)
	local team = plyr.Team
	local value
	if team == game:GetService("Teams")["Red"] then
		value = "Medium red"
	elseif team == game:GetService("Teams")["Blue"] then
		value = "Pastel Blue"
	elseif team == game:GetService("Teams")["Green"] then
		value = "Medium green"
	elseif team == game:GetService("Teams")["Yellow"] then
		value= "Cool yellow"
	elseif team == game:GetService("Teams")["For Hire"] then
		value = "For Hire"
	end
	return value
end

buttons.give.button.MouseButton1Click:Connect(function()
	
	for _,v in pairs(Selected:GetChildren()) do
		for _, plyr in pairs(game.Players:GetPlayers()) do
			if v.Value == plyr.Name then
				repeat
					local rp = plyr.Character.HumanoidRootPart
					local p, v = rp.CFrame, rp.Velocity
					shootWand(p.x+v.x/3,p.y+v.y/3,p.z+v.z/3,-(10000))
					print("Trying to heal: "..plyr.Name)
					wait(1)
				until plyr.Character.Humanoid.Health >= 100
			end
		end
	end
	game.Players.LocalPlayer.Character.Humanoid:UnequipTools("Wand")
end)
buttons.kill.button.MouseButton1Click:Connect(function()
	
	for _,v in pairs(Selected:GetChildren()) do
		for _, plyr in pairs(game.Players:GetPlayers()) do
			if v.Value == plyr.Name then
				repeat
					local rp = plyr.Character.HumanoidRootPart
					local p, v = rp.CFrame, rp.Velocity
					shootWand(p.x+v.x/3,p.y+v.y/3,p.z+v.z/3,math.huge)
					print("Trying to kill: "..plyr.Name)
					wait(1)
				until plyr.Character.Humanoid.Health < 0.1
                
			end
		end
	end
	game.Players.LocalPlayer.Character.Humanoid:UnequipTools("Wand")
end)

local ond = false
miscButtons.clickClicker.button.MouseButton1Click:Connect(function()
	local team = teamCheck(game.Players.LocalPlayer)
	local path
	local kit = game.workspace["berezaa's Tycoon Kit"]
	if team == "For Hire" then
		return print("join a team")
	else
		path = kit[team]:FindFirstChild("Mine", true).Button.ClickDetector
	end
	ond = not ond
	if ond and team ~= "For Hire" then
		RS:BindToRenderStep(tostring("a"), 0, function()
			fireclickdetector(path)
		end) 
	else
		RS:UnbindFromRenderStep(tostring("a"))
	end
end)

miscButtons.firstRow.getGears.button.MouseButton1Click:Connect(function()
	local names = {
		"Broom","Ice Staff",
		"Korblox Staff","Lightning Staff",
		"Meteor Staff","Snow Staff",
		"Vine Staff","Wind Broom",
		"Wind Staff",
	}
	print("Starting attempts...")
	local hr = game.Players.LocalPlayer.Character.HumanoidRootPart
	local op = hr.CFrame

	for i,v in pairs(game.workspace["berezaa's Tycoon Kit"]:GetDescendants()) do
		for i2,gn in pairs(names) do
			if v.Name == gn then
				warn("\n Attempting to grab: "..v.Name)
				hr.CFrame = v.CFrame
				print("\n Attempt complete.")
				wait(0.25)
			end
		end
	end
	hr.CFrame = op
end)

miscButtons.firstRow.toggleGates.button.MouseButton1Click:Connect(function()
    gates = not gates
    if gates then
        for i,v in ipairs(game.workspace:GetDescendants()) do
            if v.Name == "Gate" then
                v.Parent = game.Lighting
            end
        end
    else
        for i,v in ipairs(game.Lighting:GetDescendants()) do
            if v.Name == "Gate" then
                v.Parent = game.workspace
            end
        end
    end
end)

fireGamepass.firstRow.killAll.button.MouseButton1Click:Connect(function()
	local lp = game.Players.LocalPlayer
	local char = lp.Character
	local op = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

	if char:FindFirstChild("Inferno") then
		local Inferno = char.Inferno
		Inferno.Fire:Destroy()
	else
		local InfernoB = lp.Backpack.Inferno
		char.Humanoid:EquipTool(InfernoB)
		local Inferno = char.Inferno
		Inferno.Fire:Destroy()
	end

	local Inferno = char.Inferno
	local AP = Inferno.ActivateSpecial

	for i,v in pairs(game.Players:GetPlayers()) do
		if v.Name ~= lp.Name then
			repeat
				task.wait()
				local hr = lp.Character.HumanoidRootPart
				local p, vel = hr.CFrame, hr.Velocity
				local x,y,z = p.x+vel.x/3, p.y+vel.y/3, p.z+vel.z/3
				hr.CFrame = v.Character.HumanoidRootPart.CFrame
				AP:FireServer(CFrame.new(x,y,z))
			until v.Character.Humanoid.Health < 0.1
		end
	end
	lp.Character.HumanoidRootPart.CFrame = op
end)

fireGamepass.firstRow.serverLagger.button.MouseButton1Click:Connect(function()
    local lp = game.Players.LocalPlayer
	local char = lp.Character
	if char:FindFirstChild("Inferno") then
		local Inferno = char.Inferno
		Inferno.Fire:Destroy()
	else
		local InfernoB = lp.Backpack.Inferno
		char.Humanoid:EquipTool(InfernoB)
		local Inferno = char.Inferno
		Inferno.Fire:Destroy()
	end

	local Inferno = char.Inferno
	serverraper = not serverraper
	local lp = game.Players.LocalPlayer
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if serverraper then
			local hr = lp.Character.HumanoidRootPart
			local p, vel = hr.CFrame, hr.Velocity
			local x,y,z = p.x+vel.x/3, p.y+vel.y/3, p.z+vel.z/3
			Inferno:FindFirstChild("ActivateSpecial"):FireServer(CFrame.new(x,y,z))
        else
            Inferno:Destroy()
            lp.Character.HumanoidRootPart:Destroy()
		end
	end)
end)

fireGamepass.clickAbility.button.MouseButton1Click:Connect(function()
    local click  = not click
	local lp = game.Players.LocalPlayer
	local m = lp:GetMouse()
	local char = lp.Character

	local InfernoB = lp.Backpack.Inferno
	local ProgressUI = InfernoB.ProgressUI
    ProgressUI.Frame.Title.Name = "griffin#8008"
    ProgressUI.Frame.Tween.Name = "griffin2"
    if ProgressUI.Frame:FindFirstChild("TextLabel") then
        ProgressUI.Frame["TextLabel"].Name = "griffin1"
    end
    ProgressUI.Frame["griffin1"].Text = "infinite"

	if char:FindFirstChild("Inferno") then
		local Inferno = char.Inferno
	else
		char.Humanoid:EquipTool(InfernoB)
		local Inferno = char.Inferno
	end
	local Inferno = char.Inferno
    Inferno.Fire:Destroy()
	local function clickA()
		if click then
			if char:FindFirstChild("Inferno") then
				Inferno.ActivateSpecial:FireServer(m.Hit)
				Inferno.ProgressUI.Frame["griffin2"].Size = UDim2.fromScale(1, 1)
			end
		else
            wait()
		end
	end
	m.Button1Up:Connect(clickA)
end)

RS:BindToRenderStep(tostring("b"), 0, function()
	for _,v in pairs(game.Players:GetPlayers()) do
		
		local content = playersContent:FindFirstChild("content")
        if not content then
            task.wait()
            break
        end
		
		for _,player in pairs(content:GetChildren()) do
			
			if player.Name == v.Name then
				
				if content:FindFirstChild(v.Name) then
					content[v.Name]:WaitForChild("playerName").TextColor = v.TeamColor
					content[v.Name]:WaitForChild("playerIcon").BackgroundColor = v.TeamColor
				end
				
			end
			
		end
		
	end
end)
